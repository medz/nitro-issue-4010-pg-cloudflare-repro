{
  "name": "issue-4010-pg-cloudflare-repro",
  "private": true,
  "type": "module",
  "scripts": {
    "nitro:build": "NITRO_BUILDER=rolldown nitro build",
    "nitro:worker:check": "CI=1 wrangler dev --config .output/server/wrangler.json --port 8787",
    "repro:nitro:fail": "pnpm run nitro:build && pnpm run nitro:worker:check",
    "wrangler:direct:dev:check": "bash ./scripts/wrangler-direct-dev-check.sh",
    "wrangler:direct:dry-run": "wrangler deploy --dry-run --config wrangler.direct.json",
    "repro:wrangler:ok": "pnpm run wrangler:direct:dev:check && pnpm run wrangler:direct:dry-run",
    "repro": "pnpm run repro:nitro:fail"
  },
  "dependencies": {
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/pg": "^8.15.6",
    "nitro": "latest",
    "wrangler": "^4.63.0"
  }
}
